rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2021, 9, 12);
    }
    match/users/{uid}/{document=**}{
      allow read,write:if request.auth.uid==uid;
    }
  }
}